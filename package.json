    "pg": "^8.18.0",
    "runed": "^0.37.1",
    "svelte-toolbelt": "^0.10.6",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@lucide/svelte": "^0.575.0",
    "@number-flow/svelte": "^0.3.11",
    "@prettier/plugin-oxc": "^0.1.3",
    "@svelte-put/qr": "^2.1.1",
