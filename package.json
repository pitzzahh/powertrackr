{
  "name": "powertrackr",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "build:preview": "npm run build && npm run preview",
    "prepare": "svelte-kit sync || echo ''",
    "postinstall": "git config core.hooksPath .githooks 2>/dev/null || echo 'Git hooks setup skipped'",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "oxlint .",
    "format": "prettier --write .",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx src/lib/server/db/migrate.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.1",
    "@libsql/client": "^0.17.0",
    "@oslojs/binary": "^1.0.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@pilcrowjs/object-parser": "^0.0.4",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "arctic": "^3.7.0",
    "argon2": "^0.44.0",
    "d3-shape": "^3.2.0",
    "dotenv": "^17.2.3",
    "mode-watcher": "^1.1.0",
    "pg": "^8.17.2",
    "runed": "^0.37.1",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@lucide/svelte": "^0.562.0",
    "@prettier/plugin-oxc": "^0.1.3",
    "@sveltejs/adapter-node": "^5.5.1",
    "@sveltejs/enhanced-img": "^0.9.3",
    "@sveltejs/kit": "^2.50.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/table-core": "^8.21.3",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.8",
    "@types/pg": "^8.16.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "bits-ui": "^2.15.4",
    "clsx": "^2.1.1",
    "drizzle-kit": "^1.0.0-beta.9-e89174b",
    "drizzle-orm": "^1.0.0-beta.9-e89174b",
    "layerchart": "2.0.0-next.43",
    "oxlint": "^1.41.0",
    "pg-mem": "^3.0.5",
    "prettier": "^3.8.0",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte": "^5.47.1",
    "svelte-check": "^4.3.5",
    "svelte-sonner": "^1.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  }
}
