# Database Configuration
# Turso Database URL (your remote database)
DATABASE_URL=libsql://your-database.turso.io

# Turso Authentication Token
DATABASE_AUTH_TOKEN=your-auth-token-here

# Encryption key for password hashing, DO NOT LOSE THIS
# Generate with: openssl rand -hex 32
ENCRYPTION_KEY=your-encryption-key-here

# OAuth Configuration
# GitHub OAuth (if using GitHub authentication)
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Email Configuration
PLUNK_SECRET_KEY=your-plunk-secret-key
